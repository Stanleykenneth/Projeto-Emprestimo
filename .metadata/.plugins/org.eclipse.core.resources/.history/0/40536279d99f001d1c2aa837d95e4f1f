package calcula.emprestimo;

import javax.swing.JOptionPane;

public class Funcionario extends Pessoa {

	private int registro;
	private String senha;

	Funcionario(String nome, String cpf, int data, int r, String s) {
		super(nome, cpf, data);
		registro = r;
		senha = s;
	}

	public int getRegistro() {
		return registro;
	}

	public void setRegistro(int registro) {
		this.registro = registro;
	}

	public String getSenha() {
		return senha;
	}

	public void setSenha(String senha) {
		this.senha = senha;
	}

	@Override
	public boolean validar() {
		if (registro == 1234 && senha.equals("a1b2c3")) {
			return true;
		} else {
			JOptionPane.showMessageDialog(null, "Registro ou senha inv√°lidos");
			return false;
		}
	}

	@Override
	public String toString() {
		return "Bem vindo ao sistema " + super.getNome();

	}

}
